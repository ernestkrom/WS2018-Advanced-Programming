1.a
    grep [0-9] faust.txt
1.b
    grep ^[0-9] faust.txt
1.c
    grep ^_ faust.txt
1.d
    grep "^_.*_$" faust.txt
1.e
    grep  "Strom" faust.txt

2.a
    wc -l faust.txt
2.b
    grep "^_.*_$" faustutf.txt | uniq | wc -l
2.c
    grep ^Und faustutf.txt | wc -l
2.d
    cut -d ' ' -f3 log0.txt | sort | uniq
    awk '{print $3}' log0.txt | sort | uniq

3.
    awk 'BEGIN {SUBSEP = " "} {col[$3,$6]++} END {for(i in col) print i, col[i]}' log0.txt | sort

4.
    sed -e 's/^[ ]*//' faust1.txt > faust1ws.txt
